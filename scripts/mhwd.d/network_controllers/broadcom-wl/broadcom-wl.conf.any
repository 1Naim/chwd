# mhwd Driver Config

INCLUDE="translatedtext"

NAME="broadcom-wl"
FREEDRIVER="true"
PREFEROVER=""
DEPENDS="broadcom-wl"
CONFLICTS=""

CLASSID="0280"
VENDORIDS="14e4"
DEVICEIDS="*"

MODULES="!b43 !ssb wl lib80211 lib80211_crypt_tkip"


default()
{
	mhwd_unload_Module b43
	mhwd_unload_Module ssb

	mhwd_load_Module wl
	mhwd_load_Module lib80211
	mhwd_load_Module lib80211_crypt_tkip
}


add_configure_hook default default
